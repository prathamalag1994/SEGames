    <td><%= sel.grank %>&nbsp;&nbsp;&nbsp;</td> 
    <td><%= sel.name %>&nbsp;&nbsp;&nbsp;</td>  
    <td><%= sel.points %>&nbsp;&nbsp;&nbsp;</td> 
    <td><%= sel.geo %></td>  
  </tr>
<% end %>                             </table> <br>
             
        </div>
  </div>

</div>










